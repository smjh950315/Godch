@model dynamic
<p>
    @Html.ActionLink("Create", "Create", "Tag")
</p>
<div class="row">
    <div class="col-sm-3">TagName</div>
    <div class="col-sm-9">TagDescription</div>
</div>
<div class="row">
    @foreach(var item in Model){
        <a class="col-sm-3" href="/Tag/Detail?tid=@item.TagId">@item.TagName</a>
        <div class="col-sm-9">@item.Description</div>
    }
</div>
<div>
    @Html.ActionLink("Back", "Index", "Home")
</div>
